{
  "autoload": {
    "psr-4": {
      "PokerHands\\": "src/"
    },
    "files": [
      "src/Functional/autoload.php",
      "src/Rules/HandsComparison/autoload.php",
      "src/Rules/RankCardGroups/autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "PokerHands\\Tests\\": "tests/"
    }
