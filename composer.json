  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "pestphp/pest": "*"
  },
  "require": {
    "php": "^8.1",
    "lambdish/phunctional": "*"
  },
  "config": {
    "allow-plugins": {"pestphp/pest-plugin": true}
  }
}